{
    "compilerOptions": {
        "sourceMap": false,
        "target": "es2016",
        "useDefineForClassFields": false,
        "module": "esnext",
        "moduleResolution": "node",
        "allowJs": false,
        "strict": true,
        "noUnusedLocals": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "removeComments": false,
        "jsx": "react",
        "jsxFactory": "h",
        "lib": [
            "esnext",
            "dom"
        ],
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "rootDir": "."
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}